  "svg": "<svg xmlns=\"http://www.w3.org/2000/svg\" viewBox=\"0 0 800 600\" width=\"800\" height=\"600\">\n  <defs>\n    <linearGradient id=\"beamGradient\" x1=\"0\" y1=\"1\" x2=\"0\" y2=\"0\">\n      <stop offset=\"0%\" stop-color=\"#ffdf88\" stop-opacity=\"0.6\"/>\n      <stop offset=\"40%\" stop-color=\"#ffdf88\" stop-opacity=\"0.2\"/>\n      <stop offset=\"100%\" stop-color=\"#ffdf88\" stop-opacity=\"0.1\"/>\n    </linearGradient>\n    <linearGradient id=\"pulseBeamGradient\" x1=\"0\" y1=\"1\" x2=\"0\" y2=\"0\">\n      <stop offset=\"0%\" stop-color=\"#ffdf88\" stop-opacity=\"0.7\"/>\n      <stop offset=\"100%\" stop-color=\"#ffdf88\" stop-opacity=\"0.3\"/>\n    </linearGradient>\n    <radialGradient id=\"glowGradient\" cx=\"0.5\" cy=\"0.5\" r=\"0.5\" fx=\"0.5\" fy=\"0.5\">\n      <stop offset=\"0%\" stop-color=\"#ffdf88\" stop-opacity=\"1\"/>\n      <stop offset=\"70%\" stop-color=\"#ffdf88\" stop-opacity=\"0.7\"/>\n      <stop offset=\"100%\" stop-color=\"#ffdf88\" stop-opacity=\"0\"/>\n    </radialGradient>\n  </defs>\n  <rect id=\"background\" x=\"0\" y=\"0\" width=\"100%\" height=\"100%\" fill=\"#0a0e14\"></rect>\n  <g id=\"stars\" opacity=\"0.7\">\n    <circle cx=\"100\" cy=\"100\" r=\"1\" fill=\"#ffffff\">\n      <animate attributeName=\"opacity\" values=\"0.3; 1; 0.3\" dur=\"7s\" repeatCount=\"indefinite\"/>\n    </circle>\n    <circle cx=\"200\" cy=\"150\" r=\"1.5\" fill=\"#ffffff\">\n      <animate attributeName=\"opacity\" values=\"0.3; 1; 0.3\" dur=\"5s\" repeatCount=\"indefinite\" begin=\"1s\"/>\n    </circle>\n    <circle cx=\"300\" cy=\"120\" r=\"1\" fill=\"#ffffff\">\n      <animate attributeName=\"opacity\" values=\"0.3; 1; 0.3\" dur=\"6s\" repeatCount=\"indefinite\" begin=\"0.5s\"/>\n    </circle>\n    <circle cx=\"550\" cy=\"180\" r=\"1.2\" fill=\"#ffffff\">\n      <animate attributeName=\"opacity\" values=\"0.3; 1; 0.3\" dur=\"9s\" repeatCount=\"indefinite\" begin=\"0.8s\"/>\n    </circle>\n    <circle cx=\"650\" cy=\"90\" r=\"1\" fill=\"#fff